{
  "1": {
    "name": "Assets Biofísicos",
    "description": "São os atributos bióticos e abióticos presentes dentro dos limites da área protegida",
    "subcategories": {
      "1.1": {
        "name": "Beleza Cênica",
        "subcategories": {
          "1.1.1": {
            "name": "Vistas e Panoramas"
          }
        }
      },
      "1.2": {
        "name": "Estruturas e Formações Naturais",
        "subcategories": {
          "1.2.1": {
            "name": "Topográfica",
            "example": "cachoeiras"
          },
          "1.2.2": {
            "name": "Geomórfica",
            "example": "cavernas, falésias, formações recifais, dunas"
          }
        }
      },
      "1.3": {
        "name": "Espetáculos Naturais",
        "subcategories": {
          "1.3.1": {
            "name": "Biótico",
            "example": "agregações reprodutivas, interações predador-presa"
          },
          "1.3.2": {
            "name": "Abiótico",
            "example": "gêiser, nascentes"
          }
        }
      },
      "1.4": {
        "name": "Recursos Naturais Renováveis",
        "subcategories": {
          "1.4.1": {
            "name": "Biodiversidade/Recursos Genéticos",
            "example": "diversidade biológica para bioprospecção ou ancestrais de animais de produção ou domésticos"
          },
          "1.4.2": {
            "name": "Produtos não madeireiros",
            "example": "plantas medicinais, alimentos, resina, frutas, fungos"
          },
          "1.4.3": {
            "name": "Madeira"
          },
          "1.4.4": {
            "name": "Caça e pesca"
          },
          "1.4.5": {
            "name": "Estoques de carbono"
          },
          "1.4.6": {
            "name": "Vento para energia eólica"
          },
          "1.4.7": {
            "name": "Rio com força para geração de energia hidráulica"
          }
        }
      },
      "1.5": {
        "name": "Recursos Naturais Não renováveis",
        "subcategories": {
          "1.5.1": {
            "name": "Minerais"
          },
          "1.5.2": {
            "name": "Óleo, Gás, Carvão"
          },
          "1.5.3": {
            "name": "Fósseis"
          }
        }
      },
      "1.6": {
        "name": "Espécies",
        "subcategories": {
          "1.6.1": {
            "name": "Espécies ou indivíduos icônicos e emblemáticos",
            "example": "animais famosos, símbolo do estado"
          },
          "1.6.2": {
            "name": "Espécies importantes para conservação",
            "example": "espécies-bandeira, raras ou endêmicas"
          },
          "1.6.3": {
            "name": "Espécies importantes para recreação",
            "example": "pesca recreativa"
          },
          "1.6.4": {
            "name": "Espécies importantes para economia"
          },
          "1.6.5": {
            "name": "Espécies importantes para cultura e/ou religião"
          }
        }
      },
      "1.7": {
        "name": "Ecossistemas",
        "subcategories": {
          "1.7.1": {
            "name": "Diversidade de Habitat/Ecossistemas"
          },
          "1.7.2": {
            "name": "Habitat de espécies altamente restritas/ameaçadas"
          },
          "1.7.3": {
            "name": "Funções do ecossistema que criam serviços ecossistêmicos de suporte",
            "example": "reciclagem de nutrientes, produção primária, formação de solos, polinização"
          },
          "1.7.4": {
            "name": "Funções do ecossistema que criam serviços ecossistêmicos de regulação",
            "example": "sequestro de carbono, ciclo hidrológico/bacia hidrográfica, decomposição, predação, ciclo populacional"
          },
          "1.7.5": {
            "name": "Corredores ecológicos/Vias verdes"
          },
          "1.7.6": {
            "name": "Serviços de bacias hidrográficas"
          },
          "1.7.7": {
            "name": "Áreas de restauração ecológica",
            "example": "Recuperação de áreas degradadas"
          }
        }
      },
      "1.8": {
        "name": "Recursos para recreação ao ar livre",
        "example": "águas cristalinas, penhasco para escaladas, rapel"
      },
      "1.9": {
        "name": "Habitats antropogênicos",
        "subcategories": {
          "1.9.1": {
            "name": "Agroflorestas"
          },
          "1.9.2": {
            "name": "Criatório de espécies animais"
          },
          "1.9.3": {
            "name": "Outros"
          }
        }
      }
    }
  },
  "2": {
    "name": "Assets Humanos",
    "description": "São os grupos de pessoas associadas com a área protegida que têm um conhecimento e/ou habilidades que proporcionam a conservação dos assets da área protegida e a geração e captura de valor dos assets",
    "subcategories": {
      "2.1": {
        "name": "Técnicos e gestores da área protegida",
        "subcategories": {
          "2.1.1": {
            "name": "Permanente"
          },
          "2.1.2": {
            "name": "Temporário"
          }
        }
      },
      "2.2": {
        "name": "Guardas-florestais e brigadistas"
      },
      "2.3": {
        "name": "Guias"
      },
      "2.4": {
        "name": "Voluntários"
      },
      "2.5": {
        "name": "Pesquisadores"
      },
      "2.6": {
        "name": "Comunidades e povos tradicionais com conhecimento ecológico local e de uso de recurso"
      },
      "2.7": {
        "name": "Visitantes regulares e entusiastas de recreação",
        "example": "observadores de aves, espeleólogos, mergulhadores"
      }
    }
  },
