  "4": {
    "name": "Assets Institucionais",
    "description": "É o quadro legal que constrói uma área protegida e as estruturas e acordos contratuais que conservam assets e viabilizam geração e captação de valor",
    "subcategories": {
      "4.1": {
        "name": "Designações adicionais de conservação",
        "subcategories": {
          "4.1.1": {
            "name": "Designação Nacional",
            "example": "SNUC, Áreas Prioritárias para Conservação da Natureza"
          },
          "4.1.2": {
            "name": "Designação Internacional",
            "example": "Ramsar, World heritage, Reserva da Biosfera"
          },
          "4.1.3": {
            "name": "Designação de ONGs",
            "example": "KBA, IBA, AZA"
          },
          "4.1.4": {
            "name": "Outros"
          }
        }
      },
      "4.2": {
        "name": "Estrutura de tomada de decisão",
        "subcategories": {
          "4.2.1": {
            "name": "Planejamento",
            "example": "plano de manejo"
          },
          "4.2.2": {
            "name": "Conselho gestor"
          },
          "4.2.3": {
            "name": "Planos de Zoneamento",
            "example": "área de amortecimento"
          },
          "4.2.4": {
            "name": "Entidades de Governança",
            "example": "Conselhos consultivos, Associações/Colônias de pesca"
          }
        }
      },
      "4.3": {
        "name": "Parcerias e acordos comerciais",
        "subcategories": {
          "4.3.1": {
            "name": "Acordos de cogestão",
            "example": "comunitários etc."
          },
          "4.3.2": {
            "name": "Contratos de Pagamento de Serviços Ecossistêmicos",
