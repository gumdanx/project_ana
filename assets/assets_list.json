          "4.3.4": {
            "name": "Acordos de concessão",
            "example": "companhias de ecoturismo e recreação, provedores de serviços para visitantes"
          },
          "4.3.5": {
            "name": "Acordos de orçamentos adicionais",
            "example": "acordo de ICMS ecológicos"
          },
          "4.3.6": {
            "name": "Acordos de pesquisa",
            "example": "parcerias com universidades"
          },
          "4.3.7": {
            "name": "Acordos de compensação",
            "example": "corporações"
          },
          "4.3.8": {
            "name": "Outros acordos",
            "example": "governança"
          }
        }
      },
      "4.4": {
        "name": "Parcerias e acordos com outras agências governamentais",
        "subcategories": {
          "4.4.1": {
            "name": "Gestão em rede",
